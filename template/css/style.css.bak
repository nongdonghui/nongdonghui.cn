.list-group-elem.active, .list-group-elem.active:focus, .list-group-elem.active:hover {
    z-index: 2;
    color: #000;
    background-color: #FFF;
    border-left: 4px solid #337ab7;
}
.list-group-elem.active:hover {
    background-color: #f5f5f5;
}
a.list-group-elem:focus, a.list-group-elem:hover, button.list-group-elem:focus, button.list-group-elem:hover {
    color: #555;
    text-decoration: none;
    background-color: #f5f5f5;
}
.list-group-elem:first-child {
    border-top-left-radius: 0px;
    border-top-right-radius: 0px;
}
a.list-group-elem, button.list-group-elem {
    color: #555;
}
a:focus, a:hover {
    color: #23527c;
    text-decoration: underline;
}
a:active, a:hover {
    outline: 0;
}
.list-group-elem {
    position: relative;
    display: block;
    padding: 2px 6px;
	height: 24px;
    margin-bottom: 2px;
    background-color: #fff;
	border: none;
    border-left: 4px solid #e0e0e0;
}